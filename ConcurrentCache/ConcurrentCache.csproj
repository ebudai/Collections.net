<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <Nullable>disable</Nullable>
    <RootNamespace>Budaisoft.Collections</RootNamespace>
    <AssemblyName>Budaisoft.Collections</AssemblyName>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <AnalysisLevel>latest</AnalysisLevel>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Copyright>Eric Budai</Copyright>
    <Description>A collection of useful Collections</Description>
    <PackageIcon>Buddha-Statue-icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/ebudai/Collections.net.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/ebudai/Collections.net.git</PackageProjectUrl>
    <PackageTags>collections concurrent</PackageTags>
    <PackageReleaseNotes>Currently, this only has a better concurrent dictionary.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
  </ItemGroup>
  
  <ItemGroup>
    <None Update="Buddha-Statue-icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  
</Project>
